{
  "name": "pawelgrzybek.com",
  "version": "2.0.0",
  "description": "My Jekyll based website pawelgrzybek.com",
  "author": "Pawel Grzybek",
  "keywords": [
    "css",
    "gulp",
    "scss",
    "sass",
    "mobile-first",
    "responsive",
    "jekyll"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/pawelgrzybek/pawelgrzybek.github.io"
  },
  "scripts": {
    "dev": "php -S 0.0.0.0:8000 -t _site/",
    "build": "npm run build-css && npm run build-jekyll",
    "build-jekyll": "bundle exec jekyll build --drafts --quiet --future",
    "build-css": "cp _src/css/style.css dist/css/style.css"
  },
  "homepage": "https://pawelgrzybek.com",
  "devDependencies": {
    "browser-sync": "2.18.13"
  }
}
